{
  "name": "gemini",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "clean": "rd /s /q dist",
    "build": "tsc",
    "start": "node dist/app.js",
    "clean-xtrem": "rd /s /q dist && rm -rf node_modules && npm cache clean --force && rm -rf package-lock.json && npm install",
    "build-cjs": "tsc && node rename-cjs.cjs",
    "start-cjs": "node dist/app.cjs",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "NeoRyu",
  "license": "ISC",
  "description": "NodeJS REST-API to interact with Google Gemini",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "eslint": "^8.57.1",
    "prettier": "3.5.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "8.32.0",
    "@typescript-eslint/parser": "8.32.0",
    "@google/generative-ai": "^0.24.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.32.0"
  },
  "type": "module"
}